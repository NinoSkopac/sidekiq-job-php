{
  "name": "spinx/sidekiq-job-php",
  "description": "Push and schedule jobs to Sidekiq from PHP",
  "keywords" : [
    "sidekiq",
    "resque",
    "redis",
    "worker",
    "queue",
    "processing"
  ],
  "type": "library",
  "require": {
    "php": ">=5.6",
    "predis/predis": "^1.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Damjan Znidarsic",
      "email": "damjan@znidarsic.me"
    }
  ],
  "autoload": {
    "psr-4": {"SidekiqJob\\": "lib/"}
  },
  "require-dev": {
    "symfony/var-dumper": "^2.7",
    "phpspec/phpspec": "^2.2",
    "henrikbjorn/phpspec-code-coverage": "^1.0"
  },
  "config": {
    "bin-dir": "bin"
  }
